@model List<Agent>
<partial name="_Notification" />

<div class="container">
 @*    <div class="card-body p-6"> *@
    <div class="row">
        <div class="col-6">
            <h1>List of Agents</h1>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Agent" asp-action="Upsert" class="btn btn-primary">
                Add Agent</>
            </a>
        </div>
    </div>
    <table id="tblData" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    Agent Name
                </th>
                <th>
                    Agent Team
                </th>
                <th>
                    
                </th>


            </tr>
        </thead>
    </table>
    <a asp-area="User" asp-controller="Home" asp-action="Index" class="btn btn-secondary w-50">Back</a>
</div>

@section Scripts{
    <script src="~/js/agent.js"></script>
}