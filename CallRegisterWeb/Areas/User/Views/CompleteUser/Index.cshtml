@model List<Agent>
<partial name="_Notification" />

<div class="container">
    <div class="row">
        <div class="col-6">
            <h1>List of Productss</h1>
        </div>

    </div>
    <table class="table table-bordered table-striped">

        <thead>
            <tr>
                <th>
                    Agent Name
                </th>
                <th>
                    Select
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model.OrderBy(u => u.Name))
            {
                <tr>
                    <td>@obj.Name</td>


                    <td>
                        <div class="w-75 btn-group" role="group">
                            <a asp-controller="CompleteUser" asp-action="Edit" asp-route-id="@obj.Name" class="btn btn-primary mx-2">
                                <i class="bi bi-pencil"></i> Select
                            </a>
                        </div>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>



